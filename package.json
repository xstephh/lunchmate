{
  "name": "lunchmate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "start": "next start -p 3000",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "test": "vitest run --config vitest.config.ts",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:seed": "prisma db seed",
    "db:studio": "prisma studio",
    "db:migrate": "prisma migrate dev",
    "db:migrate:prod": "prisma migrate deploy",
    "db:backfill": "node scripts/backfill_taxonomy.cjs",
    "db:backfill:dry": "node scripts/backfill_taxonomy.cjs --dry-run",
    "fix:imports": "node tools/fix-imports.mjs",
    "prisma:select": "node scripts/select_prisma_schema.cjs",
    "prisma:use:sqlite": "PRISMA_TARGET=sqlite pnpm prisma:select",
    "prisma:use:pg": "PRISMA_TARGET=pg pnpm prisma:select",
    "prebuild": "pnpm prisma:select && node scripts/prepare_db_for_build.cjs && prisma validate && prisma generate",
    "postinstall": "prisma generate"
  },
  "prisma": {
    "seed": "node prisma/seed.cjs"
  },
  "dependencies": {
    "@prisma/client": "^6.16.2",
    "@radix-ui/react-slot": "1.0.2",
    "class-variance-authority": "0.7.0",
    "clsx": "2.1.1",
    "lucide-react": "0.456.0",
    "next": "14.2.5",
    "next-themes": "0.3.0",
    "papaparse": "^5.5.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "recharts": "2.12.7",
    "tailwind-merge": "2.5.2",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "6.4.8",
    "@testing-library/react": "14.3.1",
    "@testing-library/user-event": "14.5.2",
    "@types/node": "20.14.9",
    "@types/papaparse": "^5.3.16",
    "@types/react": "18.2.74",
    "@types/react-dom": "18.2.24",
    "autoprefixer": "10.4.19",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.5",
    "globby": "^14.1.0",
    "jsdom": "24.1.0",
    "postcss": "8.4.39",
    "prisma": "^6.16.2",
    "tailwindcss": "3.4.10",
    "typescript": "5.4.5",
    "vite": "5.4.20",
    "vitest": "1.6.0"
  }
}
